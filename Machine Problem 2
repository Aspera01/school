using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace machine_problem_1__dicitonary_
{
    internal class Program
    {
        static void Main(string[] args)
        {
            //Dictionary<int, string> grades = new Dictionary<int, string>();
            //string[] scrolls = new string[3];
            Dictionary<string, string> entries = new Dictionary<string, string>();
            Dictionary<int, Dictionary<string, string>> address = new Dictionary<int, Dictionary<string, string>>();
            int choice = 0;

            entries.Add("dhjdfhjdf", "nametest");
            address.Add(1, entries);

            Console.WriteLine("There are " + entries.Count + " entries");
            Console.WriteLine();
;

            Console.Write("OPTIONS:" + "\n" + "[1] display entries" + "\n" + "[2] add a new entry");
            Console.WriteLine();
            choice = int.Parse(Console.ReadLine());

            switch(choice)
            {
                case 1:
                    foreach (KeyValuePair<string, string> adresses in address)
                    {
                        Console.Write(adresses.Value);
                    }
                    break;
                case 2:
                    Console.WriteLine("it worked 2");
                    break;
            }



            Console.ReadKey();
        }
    }
}


using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace stash
{
    internal class Program
    {
        static void Main(string[] args)
        {
            Dictionary<string, double> grades = new Dictionary<string, double>();
            // Key - subject
            // Value - grade for the subject

            // ways to add to the dictionary
            grades.Add("PE", 99);
            grades["Rizal"] = 95;
            grades["Writing"] = -4;
            // Note: If the key already exists in the dictionary, the old value is overwritten

            if (grades.ContainsKey("Programming 2"))
            {
                Console.WriteLine(grades["Programming 2"]);
            }
            else
            {
                Console.WriteLine("No Grade in Programming 2");
            }
            Console.WriteLine(grades["PE"]);
            Console.WriteLine();

            grades["PE"] = -1;

            foreach (KeyValuePair<string, double> kvp in grades)
            {
                Console.Write(kvp.Key + " - ");
                if (kvp.Value == -1)
                    Console.WriteLine("INC");
                else if (kvp.Value == -4)
                    Console.WriteLine("FDA");
                else
                    Console.WriteLine(kvp.Value);
            }
            Console.ReadKey();
        }
    }
}

Dictionary<string,string> entries = new Dictionary<string,string>(); 

Dictionary<int, Dictionary<string,string>> addressBook2 = new Dictionary<int, Dictionary<string,string>>();
