        static void reader()
        {
            string line = "";
            using (StreamReader sr = new StreamReader("Odin.txt"))
            {
                while ((line = sr.ReadLine()) != null)
                {
                    if (line.Length > 0)
                        lines.Add(line);
                }
            }
